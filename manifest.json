{
  "manifest_version": 2,

  "name": "Hit Tracker",
  "description": "",
  "version": "0.0.0",

  "permissions": [
    "activeTab",
    "http://www.bbc.co.uk/iplayer*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.bbc.co.uk/iplayer*"],
      "js": ["jquery.2.10.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}